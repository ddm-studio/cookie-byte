{
	"name": "ddm-studio/cookie-byte",
	"type": "statamic-addon",
	"version": "1.0.17",
	"description": "A highly customizable cookie consent manager for Statamic websites",
	"keywords": [
		"cookie consent",
		"cookie consent banner",
		"cookie consent manager",
		"cookie banner",
		"seo"
	],
	"license": "proprietary",
	"authors": [
		{
			"name": "dryven",
			"email": "hallo@dryven.at",
			"homepage": "https://dryven.at/"
		}
	],
	"support": {
		"email": "support@dryven.at"
	},
	"require": {
		"php": "^7.3 || ^8.0",
		"statamic/cms": "^3.0 || ^3.1 || ^3.2 || ^3.3 || ^3.4"
	},
	"autoload": {
		"psr-4": {
			"DDM\\CookieByte\\": "src"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"DDM\\CookieByte\\ServiceProvider"
			]
		},
		"statamic": {
			"name": "Cookie Byte",
			"description": "A highly customizable cookie consent manager for Statamic websites",
			"version": "1.0.17"
		}
	},
	"config": {
		"allow-plugins": {
			"pixelfear/composer-dist-plugin": true
		}
	}
}
